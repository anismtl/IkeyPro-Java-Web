    </context-param>
    <context-param>
        <param-name>posturl</param-name>
        <param-value>https://www.sandbox.paypal.com/cgi-bin/webscr </param-value>
    </context-param>
    <context-param>
        <param-name>business</param-name>
        <param-value>paypal@ikeypro.ca</param-value>
    </context-param>
    <context-param>
        <param-name>returnurl</param-name>
        <param-value>http://localhost:8084/Paypal/index.jsp</param-value>
    </context-param>
    <servlet-mapping>
        <servlet-name>Init</servlet-name>
        <url-pattern>/Init</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeProduits</servlet-name>
        <url-pattern>/ListeProduits</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GererProduit</servlet-name>
        <url-pattern>/GererProduit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contact</servlet-name>
        <url-pattern>/Contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ajax</servlet-name>
        <url-pattern>/Ajax</url-pattern>
    </servlet-mapping>
