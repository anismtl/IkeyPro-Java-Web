        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
        <error-page>
        <error-code>500</error-code>
        <location>/WEB-INF/erreur/erreur.jsp</location>
    </error-page>
    <error-page>
        <error-code>404</error-code>
        <location>/WEB-INF/erreur/erreur.jsp</location>
    </error-page>
</web-app>
