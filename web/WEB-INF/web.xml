    <listener>
        <listener-class>ca.ikeypro.Listener.MyHttpSessionListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Init</servlet-name>
        <servlet-class>ca.ikeypro.control.Init</servlet-class>
        <init-param>
            <param-name>jdbcDriver</param-name>
            <param-value>oracle.jdbc.OracleDriver</param-value>
        </init-param>
        <init-param>
            <param-name>dbURL</param-name>
            <param-value>jdbc:oracle:thin:@ikeypro.ca:1521:XE</param-value>
        </init-param>
        <init-param>
            <param-name>dbUserName</param-name>
            <param-value>ikeypro</param-value>
        </init-param>
        <init-param>
            <param-name>dbPassword</param-name>
            <param-value>ikeypro2019</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ListeProduits</servlet-name>
        <servlet-class>ca.ikeypro.control.ListeProduits</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GererProduit</servlet-name>
        <servlet-class>ca.ikeypro.control.GererProduit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contact</servlet-name>
        <servlet-class>ca.ikeypro.control.Contact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ajax</servlet-name>
        <servlet-class>ca.ikeypro.control.Ajax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Panier</servlet-name>
        <servlet-class>ca.ikeypro.control.Panier</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>ca.ikeypro.control.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>ca.ikeypro.control.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>ca.ikeypro.control.LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MonCompte</servlet-name>
        <servlet-class>ca.ikeypro.control.MonCompte</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Recherche</servlet-name>
        <servlet-class>ca.ikeypro.control.Recherche</servlet-class>
    </servlet>
     <context-param>
